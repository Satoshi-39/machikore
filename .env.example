# Supabase設定（街コレ用）
# Supabaseプロジェクトダッシュボードから取得
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# 環境
EXPO_PUBLIC_ENV=development

# API（オプション）
EXPO_PUBLIC_API_BASE_URL=https://api.machikore.app

# Google Places API Key
EXPO_PUBLIC_GOOGLE_PLACES_API_KEY=your-google-places-api-key

# Google OAuth 2.0 Client IDs
EXPO_PUBLIC_GOOGLE_OAUTH_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_OAUTH_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com

# Mapbox Access Token (公開トークン - 地図表示用)
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.your-mapbox-access-token

# Mapbox Download Token (シークレットトークン - ネイティブSDKダウンロード用)
RNMAPBOX_MAPS_DOWNLOAD_TOKEN=sk.your-mapbox-download-token

# Mapbox Style URL
EXPO_PUBLIC_MAPBOX_STYLE_URL=mapbox://styles/your-username/your-style-id

# Resend
RESEND_API_KEY=re_your-resend-api-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# RevenueCat
EXPO_PUBLIC_REVENUECAT_API_KEY=your-revenuecat-api-key

# PostHog
EXPO_PUBLIC_POSTHOG_API_KEY=your-posthog-api-key
EXPO_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Sentry
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn
SENTRY_AUTH_TOKEN=your-sentry-auth-token
